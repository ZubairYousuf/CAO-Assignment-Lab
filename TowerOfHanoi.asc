.data
prompt: .asciiz "Enter a number: "
part1: .asciiz "\nMove disk "
part2: .asciiz " from rod "
part3: .asciiz " to rod "
 
.text
main:
    li $v0,  4       
    la $a0,  prompt
    syscall
    li $v0,  5        
    syscall
 
    add $a0, $v0, $zero 
    li $a1, 'A'
    li $a2, 'B'
    li $a3, 'C'
 
    jal hanoi           
 
    li $v0, 10
    syscall